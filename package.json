{
  "name": "11ty-choo-test",
  "version": "1.0.0",
  "description": "Testing if 11ty and choo work well together",
  "main": "index.js",
  "scripts": {
    "build": "run-p build:*",
    "build:html": "eleventy",
    "build:js": "parcel build src/app/app*.js --out-dir dist/assets --public-url /assets",
    "develop": "run-p develop:*",
    "develop:js": "parcel watch src/app/app*.js --out-dir dist/assets --public-url /assets --no-hmr",
    "develop:html": "onchange src -- npm run build:html",
    "develop:serve": "browser-sync start --server dist --files dist",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "choo": "^6.13.1",
    "choo-devtools": "^2.5.1",
    "ramda": "^0.26.1"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.7.1",
    "browser-sync": "^2.26.3",
    "npm-run-all": "^4.1.5",
    "onchange": "^5.2.0",
    "parcel-bundler": "^1.11.0"
  }
}
